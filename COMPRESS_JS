# Uses browserfy, reactify, minifyify and uglify-js to compress
# Installed locally in node_modules
browserify /Users/freddy/projects/django-voteprov/static/js/react_components.js -t reactify -d -p [minifyify --map bundle.js.map --output bundle.js.map --uglify] > /Users/freddy/projects/django-voteprov/static/js/react_components.min.js
